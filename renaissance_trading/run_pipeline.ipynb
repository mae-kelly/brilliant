    "# =============================================================================\n",
    "# ğŸš€ MAIN RENAISSANCE TRADING PIPELINE\n",
    "# =============================================================================\n",
    "\n",
    "import asyncio\n",
    "from datetime import datetime, timedelta\n",
    "from IPython.display import display, HTML, clear_output\n",
    "import time\n",
    "\n",
    "# Global trading session variables\n",
    "TRADING_SESSION = {\n",
    "    'active': False,\n",
    "    'start_time': None,\n",
    "    'end_time': None,\n",
    "    'stats': {\n",
    "        'tokens_scanned': 0,\n",
    "        'signals_generated': 0, \n",
    "        'trades_executed': 0,\n",
    "        'total_profit': 0.0,\n",
    "        'portfolio_value': float(trading_config['STARTING_CAPITAL'])\n",
    "    }\n",
    "}\n",
    "\n",
    "async def run_trading_pipeline(duration_hours=1, target_tokens_per_day=10000):\n",
    "    \"\"\"Main trading pipeline orchestrator\"\"\"\n",
    "    \n",
    "    global TRADING_SESSION\n",
    "    \n",
    "    # Initialize session\n",
    "    TRADING_SESSION['active'] = True\n",
    "    TRADING_SESSION['start_time'] = time.time()\n",
    "    TRADING_SESSION['end_time'] = TRADING_SESSION['start_time'] + (duration_hours * 3600)\n",
    "    \n",
    "    display(HTML(f\"\"\"\n",
    "    <div style='background: linear-gradient(45deg, #1e3c72, #2a5298); padding: 20px; border-radius: 10px; color: white;'>\n",
    "        <h2>ğŸš€ Renaissance Trading Pipeline ACTIVATED</h2>\n",
    "        <p><strong>Duration:</strong> {duration_hours} hours</p>\n",
    "        <p><strong>Target:</strong> {target_tokens_per_day:,} tokens/day</p>\n",
    "        <p><strong>Starting Capital:</strong> ${TRADING_SESSION['stats']['portfolio_value']:.2f}</p>\n",
    "        <p><strong>Trading Mode:</strong> {'LIVE' if trading_config['ENABLE_REAL_TRADING'] == 'true' else 'SIMULATION'}</p>\n",
    "        <p><strong>Session Start:</strong> {datetime.now().strftime('%Y-%m-%d %H:%M:%S')}</p>\n",
    "    </div>\n",
    "    \"\"\"))\n",
    "    \n",
    "    try:\n",
    "        if renaissance_system:\n",
    "            # Initialize production system\n",
    "            success = await renaissance_system.initialize_system()\n",
    "            \n",
    "            if success:\n",
    "                print(\"âœ… Production system initialized successfully\")\n",
    "                \n",
    "                # Start autonomous trading\n",
    "                await renaissance_system.start_production_trading(duration_hours)\n",
    "                \n",
    "            else:\n",
    "                print(\"âŒ Production system initialization failed\")\n",
    "                await run_fallback_pipeline(duration_hours)\n",
    "        else:\n",
    "            print(\"âš ï¸ No production system available, running fallback...\")\n",
    "            await run_fallback_pipeline(duration_hours)\n",
    "            \n",
    "    except KeyboardInterrupt:\n",
    "        print(\"\\nğŸ›‘ Trading interrupted by user\")\n",
    "    except Exception as e:\n",
    "        print(f\"âŒ Trading pipeline error: {e}\")\n",
    "    finally:\n",
    "        TRADING_SESSION['active'] = False\n",
    "        await display_final_results()\n",
    "\n",
    "async def run_fallback_pipeline(duration_hours):\n",
    "    \"\"\"Fallback trading pipeline using available components\"\"\"\n",
    "    \n",
    "    print(\"ğŸ”„ Running fallback autonomous trading pipeline...\")\n",
    "    \n",
    "    end_time = time.time() + (duration_hours * 3600)\n",
    "    iteration = 0\n",
    "    \n",
    "    while time.time() < end_time and TRADING_SESSION['active']:\n",
    "        try:\n",
    "            iteration += 1\n",
    "            \n",
    "            # Simulate token scanning\n",
    "            tokens_scanned = np.random.randint(50, 200)\n",
    "            TRADING_SESSION['stats']['tokens_scanned'] += tokens_scanned\n",
    "            \n",
    "            # Simulate signal generation\n",
    "            signals_generated = np.random.randint(0, 5)\n",
    "            TRADING_SESSION['stats']['signals_generated'] += signals_generated\n",
    "            \n",
    "            # Simulate trading\n",
    "            if signals_generated > 0:\n",
    "                for _ in range(signals_generated):\n",
    "                    # Simulate trade outcome\n",
    "                    if np.random.random() > 0.4:  # 60% win rate\n",
    "                        profit = np.random.uniform(0.01, 0.15) * float(trading_config['MAX_POSITION_SIZE'])\n",
    "                    else:\n",
    "                        profit = -np.random.uniform(0.01, 0.05) * float(trading_config['MAX_POSITION_SIZE'])\n",
    "                    \n",
    "                    TRADING_SESSION['stats']['total_profit'] += profit\n",
